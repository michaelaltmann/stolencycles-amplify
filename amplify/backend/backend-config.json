{
  "api": {
    "scrape": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "scrape",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    },
    "stolencycles": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    },
    "matches": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "matches",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "function": {
    "scrape": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "stolencycles",
          "attributes": [
            "GraphQLAPIIdOutput",
            "GraphQLAPIEndpointOutput",
            "GraphQLAPIKeyOutput"
          ]
        }
      ]
    },
    "matches": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "api",
          "resourceName": "stolencycles",
          "attributes": [
            "GraphQLAPIIdOutput"
          ]
        }
      ]
    }
  },
  "hosting": {
    "amplifyhosting": {
      "service": "amplifyhosting",
      "providerPlugin": "awscloudformation",
      "type": "manual"
    }
  }
}